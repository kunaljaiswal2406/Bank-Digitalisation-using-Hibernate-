<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="bankhibernate">
    <class name="UserAccountDetails" table="UserAccountDetails">
        <id name="accountNo" column="accountNo"></id>
        <property name="id" column="id"></property>
        <property name="password" column ="password"></property>
        <property name="name" column="name"></property>
        <property name="contact" column="contact"></property>
        <property name="gender" column ="gender"></property>
        <property name="email" column ="email"></property>
        <property name="address" column="address"></property>
        <property name="balance" column="balance"></property>

    </class>

</hibernate-mapping>
